{
  "expo": {
    "name": "skin-app",
    "slug": "skin-app",
    "version": "1.0.0",
    "scheme": "skinapp",
    "devClient": false,
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.betterskin.app"
    },
    "android": {
      "package": "com.betterskin.app",
      "permissions": [
        "android.permission.CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#E6F4FE"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "skinapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cf053d99-a5af-48ff-95c1-5be97fde7ac1"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
